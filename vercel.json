{
  "version": 2,
  "builds": [
    {
      "src": "client/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "server/src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/src/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/src/$1"
    }
  ]
}